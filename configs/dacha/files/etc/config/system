config system
	option hostname 'dacha'
	option timezone 'MSK-3'
	option ttylogin '0'
	option log_file '/var/log/messages'
	option log_size 1024
	option urandom_seed '0'

config timeserver 'ntp'
	option enabled '1'
	option enable_server '0'
	list server '0.ru.pool.ntp.org'
	list server '1.ru.pool.ntp.org'
	list server '2.ru.pool.ntp.org'
	list server '3.ru.pool.ntp.org'

config led 'led_usb'
	option name 'USB'
	option sysfs 'tp-link:green:3g'
	option trigger 'netdev'
	option dev 'eth1'
	option mode 'link tx rx'

config led 'led_wlan'
	option name 'WLAN'
	option sysfs 'tp-link:green:wlan'
	option trigger 'phy0tpt'

config led 'led_lan'
	option name 'LAN'
	option sysfs 'tp-link:green:lan'
	option trigger 'netdev'
	option mode 'link tx rx'
	option dev 'eth0'

config led 'led_wps'
	option name 'WPS'
	option sysfs 'tp-link:green:wps'
	option trigger 'none'
	option default '0'
